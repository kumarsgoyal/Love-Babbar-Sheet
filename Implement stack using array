class MyStack
{
    int top;
	int arr[] = new int[101];

    MyStack() {
		top = -1;
	}
	
	//Function to push an integer into the stack.
    void push(int a) {
	    arr[++top] = a;
	} 
	
    //Function to remove an item from top of the stack.
	int pop() {
        int temp = -1;
        if(top != -1) {
            temp = arr[top--];
        }
        return temp;
	}
}
